<h2>Sign in</h2>

<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <%= f.error_notification %>

  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.submit "Sign In", class: "btn btn-primary btn-md" %></br>
<% end %>
</br>
<%= render "users/shared/links" %>
